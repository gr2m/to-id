{
  "name": "to-id",
  "description": "Normalises strings for comparison or usage as IDs",
  "main": "to-id.js",
  "scripts": {
    "test": "standard && testmate test/*.js",
    "test:node": "testmate --client node test/*.js",
    "test:browser": "testmate --client selenium:firefox test/*.js",

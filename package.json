    "postpublish": "semantic-release post",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gr2m/to-id.git"
  },
  "author": "Gregor Martynus <gregor@martynus.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gr2m/to-id/issues"
  },
  "homepage": "http://gr2m.github.io/to-id",
  "devDependencies": {
    "semantic-release": "4.3.5",
    "standard": "5.3.1",
    "testmate": "1.0.1"
  }
}
